generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Question {
  id          Int     @id @default(autoincrement())
  companyId   String  @default(uuid())
  question    String
  answer      String
  properties  String?
  description String?

  createdBy String
  updatedBy String?
  asignedTo String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
